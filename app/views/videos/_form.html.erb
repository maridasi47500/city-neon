<%= form_with(model: video,html:{onsubmit: "submitvid();return false;"}) do |form| %>
  <% if video.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(video.errors.count, "error") %> prohibited this video from being saved:</h2>

      <ul>
        <% video.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :filename, style: "display: block" %>
    <%= form.file_field :filename,id:"file", accept:"video/*",change:"processFile(file)" %>
  </div>

  <div>
    <%= form.hidden_field :user_id,id:"userid" %>
    <%= form.hidden_field :mychannel,id:"mychannel" %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>

<%= form_with(model: music,html:{onsubmit: "submitmusic();return false;"}) do |form| %>
  <% if music.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(music.errors.count, "error") %> prohibited this music from being saved:</h2>

      <ul>
        <% music.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :filename, style: "display: block" %>
    <%= form.file_field :filename, id: "file", accept:"audio/*",change:"processFile(file)" %>
    <%= form.hidden_field :user_id, id: "userid" %>

  </div>
       <%= form.hidden_field :myradio,id:"myradio" %>

  <div>
    <%= form.submit %>
  </div>
<% end %>
